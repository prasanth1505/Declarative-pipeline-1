pipeline {
    agent any

    stages {
        stage('Code') {
            steps {
                echo 'Hello World' 
                echo 'welcome Jenkins'
            }
        }
        stage('Build') {
            steps {
                echo 'Build app'
            }
        }
        stage('Test') {
            steps {
                echo 'Test app'
            }
        }
        stage('Release') {
            steps {
                echo 'Release app'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy app'
            }
        }
        stage('Operate') {
            steps {
                echo 'Operate app'
            }
        }
        stage('Monitor') {
            steps {
                echo 'Monitor app'
                echo  'Maintainance'
            }
        }
    }
}
